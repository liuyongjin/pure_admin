{
  "name": "pure_admin",
  "version": "1.0.0",
  "description": "pure admin template",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build:prod": "vue-cli-service build"
  },
  "dependencies": {
    "axios": "0.18.1",
    "core-js": "3.6.5",
    "driver.js": "0.9.5",
    "element-ui": "2.13.2",
    "fuse.js": "3.4.4",
    "js-cookie": "2.2.0",
    "normalize.css": "7.0.0",
    "nprogress": "0.2.0",
    "particles.js": "^2.0.0",
    "path-to-regexp": "2.4.0",
    "screenfull": "4.2.0",
    "vue": "3.0.0",
    "vue-i18n": "^8.8.2",
    "vue-router": "3.0.2",
    "vuex": "3.1.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "4.4.4",
    "@vue/cli-plugin-eslint": "4.4.4",
    "@vue/cli-service": "4.4.4",
    "autoprefixer": "9.5.1",
    "babel-eslint": "10.1.0",
    "babel-jest": "23.6.0",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "eslint": "6.7.2",
    "eslint-plugin-vue": "6.2.2",
    "html-webpack-plugin": "3.2.0",
    "husky": "1.3.1",
    "mockjs": "1.0.1-beta3",
    "sass": "1.26.2",
    "sass-loader": "8.0.2",
    "svg-sprite-loader": "4.1.3",
    "vue-template-compiler": "2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": [
        0,
        false
      ],
      "no-unused-vars": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">= 8.9",
    "npm": ">= 3.0.0"
  }
}
